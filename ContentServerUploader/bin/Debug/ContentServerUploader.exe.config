<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="UserName" value="Admin"/>
    <add key="Password" value="livelink"/>
  </appSettings>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_Authentication" />
                <binding name="BasicHttpBinding_DocumentManagement" />
                <binding name="BasicHttpBinding_ContentService" messageEncoding="Mtom" transferMode="Streamed" maxReceivedMessageSize="2147483648" sendTimeout="02:00:00"/>                                    
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://jangid-pc/cws/Authentication.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_Authentication" contract="CWS.Authentication"
                name="BasicHttpBinding_Authentication" />
            <endpoint address="http://jangid-pc/cws/DocumentManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_DocumentManagement"
                contract="CWS.DocumentManagement" name="BasicHttpBinding_DocumentManagement" />
            <endpoint address="http://jangid-pc/cws/ContentService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ContentService" contract="CWS.ContentService"
                name="BasicHttpBinding_ContentService" />
        </client>
    </system.serviceModel>
</configuration>